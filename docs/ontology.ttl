@prefix : <http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#> .

<http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto> rdf:type owl:Ontology ;
                                                                    <http://purl.org/dc/terms/abstract> "La ontología tiene como objetivo principal ayudar a turistas y personas que viajen por negocios a encontrar los medios de transporte, hoteles, eventos, atracciones turísticas y restaurantes que hay disponibles en una ciudad específica."@es ,
                                                                                                        "The main objective of the ontology is to help tourists and business travelers find the means of transportation, hotels, events, tourist attractions, and restaurants available in a specific city."@en ;
                                                                    <http://purl.org/dc/terms/created> "2025-04-06"^^xsd:date ;
                                                                    <http://purl.org/dc/terms/creator> "rgomez" ;
                                                                    <http://purl.org/dc/terms/description> "La ontología tiene como objetivo principal ayudar a turistas y personas que viajen por negocios a encontrar los medios de transporte, hoteles, eventos, atracciones turísticas y restaurantes que hay disponibles en una ciudad específica."@es ,
                                                                                                           "The main objective of the ontology is to help tourists and business travelers find the means of transportation, hotels, events, tourist attractions, and restaurants available in a specific city."@en ;
                                                                    <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/" ;
                                                                    <http://purl.org/dc/terms/title> "SmartTourOnto" ;
                                                                    owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/abstract
<http://purl.org/dc/terms/abstract> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#eventIsOfType
:eventIsOfType rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :typeOfEvent ;
               rdfs:domain :Event ;
               rdfs:range :EventType ;
               rdfs:comment "Especifica el tipo al que pertenece un evento"@es ,
                            "Specifies the type to which an event belongs"@en ;
               rdfs:label "event is of type"@en ,
                          "evento de tipo"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#geoCoordinatesOf
:geoCoordinatesOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf :hasGeoCoordinates ;
                  rdfs:domain :GeoCoordinate ;
                  rdfs:range :City ;
                  rdfs:comment "Indica la ciudad a la que pertenece un conjunto de coordenadas geográficas"@es ,
                               "Indicates the city to which a set of geographic coordinates belongs"@en ;
                  rdfs:label "coordenadas geográficas de"@es ,
                             "geographic coordinates of"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasGeoCoordinates
:hasGeoCoordinates rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :City ;
                   rdfs:range :GeoCoordinate ;
                   rdfs:comment "Indica el conjunto de coordenadas geográficas asociadas a una ciudad"@es ,
                                "Indicates the set of geographic coordinates associated with a city"@en ;
                   rdfs:label "has geographic coordinates"@en ,
                              "tiene coordenadas geográficas"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :locationOf ;
             rdfs:domain :Event ;
             rdfs:range :City ;
             rdfs:comment "Indica el evento que tiene lugar en una ciudad"@es ,
                          "Indicates the event that takes place in a city"@en ;
             rdfs:label "has location"@en ,
                        "tiene lugar en"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hotelIsOfType
:hotelIsOfType rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :hotelTypeOf ;
               rdfs:domain :Hotel ;
               rdfs:range :HotelType ;
               rdfs:comment "Especifica el tipo al que un hotel pertenece"@es ,
                            "Specifies the type to which a hotel belongs"@en ;
               rdfs:label "hotel es de tipo"@es ,
                          "hotel is of type"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hotelTypeOf
:hotelTypeOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :HotelType ;
             rdfs:range :Hotel ;
             rdfs:comment "Enumera los hoteles que son de un tipo de hotel determinado"@es ,
                          "Lists the hotels that are of a certain hotel type"@en ;
             rdfs:label "hotel type of"@en ,
                        "tipo de hotel de"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#includes
:includes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :isLocatedIn ;
          rdfs:domain :City ;
          rdfs:range :NonAdministrativePlace ;
          rdfs:comment "Indica los lugares que están incluidos dentro de una ciudad"@es ,
                       "Indicates the places that are included within a city"@en ;
          rdfs:label "includes"@en ,
                     "incluye"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :NonAdministrativePlace ;
             rdfs:range :City ;
             rdfs:comment "Indica la ciudad en la que se encuentra un lugar"@es ,
                          "Indicates the city in which a place is located"@en ;
             rdfs:label "está situado en"@es ,
                        "is located in"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#isOfferedIn
:isOfferedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :offersService ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Service ;
             rdfs:range :City ;
             rdfs:comment "Relaciona un servicio (dominio) con la ciudad (rango) donde dicho servicio se ofrece o está disponible"@es ,
                          "Relates a service (domain) to the city (range) where that service is offered or available"@en ;
             rdfs:label "is offered in"@en ,
                        "se ofrece en"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#locationOf
:locationOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :City ;
            rdfs:range :Event ;
            rdfs:comment "Indica la ciudad en la que tiene lugar un evento"@es ,
                         "Indicates the city where an event takes place"@en ;
            rdfs:label "location of"@en ,
                       "ubicación de"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#offersService
:offersService rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :City ;
               rdfs:range :Service ;
               rdfs:comment "List services offered in a city"@en ,
                            "Relaciona los servicios que ofrece una ciudad"@es ;
               rdfs:label "offers service"@en ,
                          "ofrece servicio"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#operatedBy
:operatedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :providesTransportService ;
            rdfs:domain :TransportService ;
            rdfs:range :TransportCompany ;
            rdfs:comment "Indica la compañía que opera un servicio de transporte"@es ,
                         "Indicates the company that operates a transport service"@en ;
            rdfs:label "Operado por"@es ,
                       "Operated by"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#providesTransportService
:providesTransportService rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain :TransportCompany ;
                          rdfs:range :TransportService ;
                          rdfs:comment "Indica el servicio de transporte que una compañía ofrece"@es ,
                                       "Indicates the transport service that a company offers"@en ;
                          rdfs:label "Proporciona servicio de transporte"@es ,
                                     "Provides transport service"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#servesSpecialty
:servesSpecialty rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf :specializedIn ;
                 rdfs:domain :Restaurant ;
                 rdfs:range :FoodType ;
                 rdfs:comment "Indica el restaurante que sirve esta especialidad de comida"@es ,
                              "Indicates the restaurant that serves this food specialty"@en ;
                 rdfs:label "Serves Specialty"@en ,
                            "Sirve especialidad"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#specializedIn
:specializedIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :FoodType ;
               rdfs:range :Restaurant ;
               rdfs:comment "Indica la especialidad culinaria por la que un restaurante es conocido"@es ,
                            "Indicates the food specialty a restaurant is known for serving"@en ;
               rdfs:label "especializado en"@es ,
                          "specialized in"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#typeOfEvent
:typeOfEvent rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :EventType ;
             rdfs:range :Event ;
             rdfs:comment "Lista los eventos que pertenecen a un tipo de evento determinado"@es ,
                          "Lists the events that belong to a certain event type"@en ;
             rdfs:label "tipo de evento"@es ,
                        "type of event"@en .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#enumerationDescription
:enumerationDescription rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain :TypeEnumeration ;
                        rdfs:range xsd:string ;
                        rdfs:comment "A textual description of an enumeration type"@en ,
                                     "Una descripción textual de un tipo de enumeración"@es ;
                        rdfs:label "descripción de la enumeración"@es ,
                                   "enumeration description"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#geolocationDescription
:geolocationDescription rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain :Geolocation ;
                        rdfs:range xsd:string ;
                        rdfs:comment "A textual description of a geographic location"@en ,
                                     "Una descripción textual de una ubicación geográfica"@es ;
                        rdfs:label "descripción de geolocalización"@es ,
                                   "geolocation description"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasAreaInSquareMeters
:hasAreaInSquareMeters rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain :Place ;
                       rdfs:range xsd:float ;
                       rdfs:comment "El área del lugar, medida en metros cuadrados"@es ,
                                    "The area of the place, measured in square meters"@en ;
                       rdfs:label "has area in square meters"@en ,
                                  "tiene área en metros cuadrados"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasAveragePrice
:hasAveragePrice rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :TransportService ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:onDatatype xsd:decimal ;
                              owl:withRestrictions ( [ xsd:minInclusive 0
                                                     ]
                                                     [ xsd:fractionDigits 2
                                                     ]
                                                   )
                            ] ;
                 rdfs:comment "Indica el precio promedio de un servicio de transporte"@es ,
                              "Indicates the average price of a transport service"@en ;
                 rdfs:label "has average price"@en ,
                            "tiene un precio medio"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasAveragePricePerPerson
:hasAveragePricePerPerson rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain :Restaurant ;
                          rdfs:range [ rdf:type rdfs:Datatype ;
                                       owl:onDatatype xsd:int ;
                                       owl:withRestrictions ( [ xsd:minInclusive "0"^^xsd:int
                                                              ]
                                                            )
                                     ] ;
                          rdfs:comment "Indica el precio promedio por persona en un restaurante"@es ,
                                       "Indicates the average price per person in a restaurant"@en ;
                          rdfs:label "has average price per person"@en ,
                                     "tiene un precio medio por persona"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasCompanyName
:hasCompanyName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain :Company ;
                rdfs:range xsd:string ;
                rdfs:comment "Especifica el nombre de una empresa"@es ,
                             "Specifies the name of a company"@en ;
                rdfs:label "has company name"@en ,
                           "tiene nombre de empresa"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasEventDate
:hasEventDate rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Event ;
              rdfs:range xsd:date ;
              rdfs:comment "La fecha en que ocurre un evento"@es ,
                           "The date on which an event occurs"@en ;
              rdfs:label "event date"@en ,
                         "fecha del evento"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasPricePerNight
:hasPricePerNight rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Accommodation ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:onDatatype xsd:decimal ;
                               owl:withRestrictions ( [ xsd:minInclusive 0
                                                      ]
                                                      [ xsd:fractionDigits 2
                                                      ]
                                                    )
                             ] ;
                  rdfs:comment "El precio por noche de un alojamiento"@es ,
                               "The price per night of an accommodation"@en ;
                  rdfs:label "Has price per night"@en ,
                             "Tiene precio por noche"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasRating
:hasRating rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :NonAdministrativePlace ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:decimal ;
                        owl:withRestrictions ( [ xsd:minInclusive 0.0
                                               ]
                                               [ xsd:maxInclusive 5.0
                                               ]
                                               [ xsd:fractionDigits "1"^^xsd:int
                                               ]
                                             )
                      ] ;
           rdfs:comment "La calificación numérica de una entidad"@es ,
                        "The numerical rating of an entity"@en ;
           rdfs:label "Has rating"@en ,
                      "Tiene calificación"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasSchedule
:hasSchedule rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Service ;
             rdfs:range xsd:string ;
             rdfs:comment "El horario en que el servicio está disponible"@es ,
                          "The schedule when the service is available"@en ;
             rdfs:label "has schedule"@en ,
                        "tiene horario"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#hasStarRating
:hasStarRating rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Accommodation ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:onDatatype xsd:int ;
                            owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:int
                                                   ]
                                                   [ xsd:maxInclusive "5"^^xsd:int
                                                   ]
                                                   [ xsd:totalDigits "1"^^xsd:int
                                                   ]
                                                 )
                          ] ;
               rdfs:comment "La calificación de estrellas de un alojamiento"@es ,
                            "he star rating of an accommodation"@en ;
               rdfs:label "Has star rating"@en ,
                          "Tiene calificación de estrellas"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#isRecommendedAccommodation
:isRecommendedAccommodation rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf owl:topDataProperty ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain :Accommodation ;
                            rdfs:range xsd:boolean ;
                            rdfs:comment "Indica si un alojamiento es recomendado"@es ,
                                         "Indicates whether an accommodation is recommended"@en ;
                            rdfs:label "es un alojamiento recomendado"@es ,
                                       "is recommended accommodation"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#isRecommendedRestaurant
:isRecommendedRestaurant rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain :Restaurant ;
                         rdfs:range xsd:boolean ;
                         rdfs:comment "Indica si un restaurante es recomendado"@es ,
                                      "Indicates whether a restaurant is recommended"@en ;
                         rdfs:label "es un restaurante recomendado"@es ,
                                    "is recommended restaurant"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#latitude
:latitude rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :GeoCoordinate ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:onDatatype xsd:decimal ;
                       owl:withRestrictions ( [ xsd:minInclusive -90.0
                                              ]
                                              [ xsd:maxInclusive 90.0
                                              ]
                                            )
                     ] ;
          rdfs:comment "La latitud de una ubicación geográfica"@es ,
                       "The latitude of a geographic location"@en ;
          rdfs:label "Latitud"@es ,
                     "Latitude"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#longitude
:longitude rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :GeoCoordinate ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:decimal ;
                        owl:withRestrictions ( [ xsd:minInclusive -180.0
                                               ]
                                               [ xsd:maxInclusive 180.0
                                               ]
                                             )
                      ] ;
           rdfs:comment "La longitud de una ubicación geográfica"@es ,
                        "The longitude of a geographic location"@en ;
           rdfs:label "Longitud"@es ,
                      "Longitude"@en .


###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:subPropertyOf owl:topDataProperty .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#Accommodation
:Accommodation rdf:type owl:Class ;
               rdfs:subClassOf :Business ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasStarRating ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:int
                               ] ;
               rdfs:comment "Representa un lugar donde se ofrece alojamiento temporal"@es ,
                            "Represents a place where temporary lodging is offered"@en ;
               rdfs:label "Accommodation"@en ,
                          "Alojamiento"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#AdministrativePlace
:AdministrativePlace rdf:type owl:Class ;
                     rdfs:subClassOf :Place ;
                     rdfs:comment "Representa un lugar geográfico con funciones administrativas o límites definidos"@es ,
                                  "Represents a geographic place with administrative functions or defined boundaries"@en ;
                     rdfs:label "Administrative place"@en ,
                                "Lugar administrativo"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#Bakery
:Bakery rdf:type owl:Class ;
        rdfs:subClassOf :FoodEstablishment ;
        rdfs:comment "Representa un establecimiento donde se hornean y venden productos de panadería"@es ,
                     "Represents an establishment where bakery products are baked and sold"@en ;
        rdfs:label "Bakery"@en ,
                   "Panadería"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :TouristAttraction ;
       rdfs:comment "Represents a stretch of sandy or pebbled coastline"@en ,
                    "Representa una extensión de costa arenosa o pedregosa" ;
       rdfs:label "Beach"@en ,
                  "Playa"@es ;
       rdfs:sameAs "https://dbpedia.org/page/Beach" .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :PublicTransport ;
     rdfs:comment "Representa un vehículo automotor de gran capacidad destinado al transporte público colectivo de pasajeros por carretera, siguiendo rutas generalmente fijas"@es ,
                  "Represents a large capacity motor vehicle intended for collective public passenger transport by road, usually following fixed routes"@en ;
     rdfs:label "Autobús"@es ,
                "Bus"@en .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :NonAdministrativePlace ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRating ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:decimal
                          ] ;
          rdfs:comment "Representa una entidad comercial o establecimiento que ofrece bienes o servicios"@es ,
                       "Represents a business entity or establishment that offers goods or services"@en ;
          rdfs:label "Business"@en ,
                     "Negocios"@es .


###  http://www.semanticweb.org/rafag/ontologies/2025/3/SmartTourOnto#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :AdministrativePlace ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasGeoCoordinates ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :GeoCoordinate
                      ] ;
